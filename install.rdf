<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" 
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">
    <Description about="urn:mozilla:install-manifest">
        <em:id>tbsync@jobisoft.de</em:id>
        <em:type>2</em:type> <!-- type=extension --> 
        <em:optionsURL>chrome://tbsync/content/manager/addonoptions.xul</em:optionsURL>
        <em:bootstrap>true</em:bootstrap>
        <em:unpack>false</em:unpack>
        <em:multiprocessCompatible>true</em:multiprocessCompatible>
        <em:name>TbSync</em:name>
        <em:version>0.7.16.8</em:version>
        <em:description>Synchronize contacts, tasks and calendars via Exchange ActiveSync or CalDAV/CardDAV to Thunderbird</em:description>
        <em:creator>John Bieling</em:creator>
        <em:homepageURL>https://github.com/jobisoft/TbSync</em:homepageURL>
        <em:iconURL>chrome://tbsync/skin/tbsync.png</em:iconURL>

        <em:targetApplication>
          <!-- Thunderbird -->
          <Description>
            <em:id>{3550f703-e582-4d05-9a08-453d09bdfdc6}</em:id>
            <em:minVersion>50.0</em:minVersion>
            <em:maxVersion>61.*</em:maxVersion>
          </Description>
        </em:targetApplication>

        <em:localized>
          <Description>
            <em:locale>de</em:locale>
            <em:name>TbSync</em:name>
            <em:description>Synchronisiert Kontakte, Aufgaben und Kalender über Exchange ActiveSync oder CalDAV/CardDAV mit Thunderbird</em:description>
          </Description>
        </em:localized>
        <em:localized>
          <Description>
            <em:locale>it-IT</em:locale>
            <em:name>TbSync</em:name>
            <em:description>Sincronizza contatti, attività e calendari in Thunderbird tramite Exchange ActiveSync o CalDAV/CardDAV</em:description>
          </Description>
        </em:localized>

    </Description>
</RDF>
